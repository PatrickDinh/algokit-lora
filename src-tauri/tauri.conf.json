{
  "productName": "algokit-lora",
  "version": "0.1.0",
  "identifier": "dev.algokit-lora.algorand-foundation",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "algokit-lora",
        "width": 1400,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "deep-link": {
      "mobile": [],
      "desktop": {
        "schemes": [
          "algokit-lora"
        ]
      }
    },
    "cli": {
      "description": "algokit-lora"
    },
    "updater": {
      "endpoints": [
        "https://cdn.crabnebula.app/update/patrick-dev/algokit-lora-dev/{{target}}-{{arch}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJGQTY2NUNBRDQ3QjlCRjQKUldUMG0zdlV5bVdtdjZmVXhaQS92RytrNWZsZHRHU2FQTE8xMnA1ajUwZDdmQUovOVVSMWFKVFgK"
    }
  }
}
